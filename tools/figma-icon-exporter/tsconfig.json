{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "jsx": "react",
    "outDir": "./",
    "typeRoots": ["./node_modules/@types", "./node_modules/@figma"],
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./*.ts"],
  "exclude": ["node_modules"]
}
