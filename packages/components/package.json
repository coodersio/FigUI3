    "@radix-ui/react-checkbox": "^1.3.3",
    "clsx": "^2.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@vitejs/plugin-react": "^4.2.1",
    "jsdom": "^23.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.7.0"
  },
  "keywords": [
    "components",
