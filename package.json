{
  "name": "@figui3/root",
  "version": "0.0.0",
  "private": true,
  "description": "FigUI3 Design System - Figma's UI Kit implementation",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean && rm -rf node_modules",
    "storybook": "pnpm --filter @figui3/storybook dev",
    "build:storybook": "pnpm --filter @figui3/storybook build",
    "version": "lerna version",
    "publish": "lerna publish"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "lerna": "^8.0.0",
    "prettier": "^3.1.1",
    "turbo": "^1.11.2",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "packageManager": "pnpm@8.12.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "@radix-ui/react-slider@1.3.6": "patches/@radix-ui__react-slider@1.3.6.patch"
    }
  }
}
